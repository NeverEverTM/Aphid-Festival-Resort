; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Aphid Festival Resort"
config/description="Aphid Festival Demo"
config/version="0.2.1.0"
run/main_scene="res://scenes/menu.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://sprites/ui/leaf_cover.png"
config/windows_native_icon="res://sprites/icon.ico"

[autoload]

GameManager="*res://scripts/GameManager.cs"
OptionsManager="*res://scripts/controllers/OptionsManager.cs"
SoundManager="*res://scripts/controllers/SoundManager.cs"
Cheats="*res://scripts/misc/Cheats.cs"

[display]

window/size/mode=2
window/energy_saving/keep_screen_on=false
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="Aphid Festival"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/glowbutton/plugin.cfg")

[file_customization]

folder_colors={
"res://databases/": "blue",
"res://databases/items/": "orange",
"res://databases/skins/": "teal",
"res://scenes/": "gray",
"res://scripts/": "red",
"res://sfx/": "pink",
"res://sprites/": "purple"
}

[filesystem]

import/blender/enabled=false

